[Unit]
Description=Delay Tracker
After=network.target

[Service]
User=crowd-sourced-realtime
Group=nogroup
Restart=always
ExecStart=/usr/local/bin/delay-tracker
WorkingDirectory=/var/cache/transitous/out/delay-tracker/

[Install]
WantedBy=multi-user.target
